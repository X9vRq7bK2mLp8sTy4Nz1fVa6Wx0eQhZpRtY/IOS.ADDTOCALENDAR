{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/resources/(.*)",
      "dest": "/resources/$1",
      "headers": {
        "Content-Type": "application/vnd.openxmlformats-officedocument.spreadsheetml.sheet"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
